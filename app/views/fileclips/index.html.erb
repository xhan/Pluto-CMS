<h1>Listing fileclips</h1>

<table>
  <tr>
    <th>F file name</th>
    <th>F content type</th>
    <th>F file size</th>
    <th>F updated at</th>
  </tr>

<% @fileclips.each do |fileclip| %>
  <tr>
    <td><%=h fileclip.f_file_name %></td>
    <td><%=h fileclip.f_content_type %></td>
    <td><%=h fileclip.f_file_size %></td>
    <td><%=h fileclip.f_updated_at %></td>
    <td><%= link_to 'Show', fileclip %></td>
    <td><%= link_to 'Edit', edit_fileclip_path(fileclip) %></td>
    <td><%= link_to 'Destroy', fileclip, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New fileclip', new_fileclip_path %>